msgid "%a1 and %a2 shared %a3's %a4."
msgstr "%a1 e %a2 lollinii %a3's %a4."

#. %a1, %a2, and %a3 are names of people
msgid "%a1 and %a2 were tagged in %a3's photos."
msgstr "%a1 e %a2 maantaama e nate %a3."

