msgid "1 NEW STORY"
msgid_plural "%d NEW STORIES"
msgstr[0] "UNA HISTORIA NUEVA"
msgstr[1] "%d HISTORIAS NUEVAS"

#. %d stands for a number, no need to translate it; "%d other people"
