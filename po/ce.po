msgid "%a1 and %a2 like this."
msgstr "%a1, %a2 a хазахета хlара."

# form of Chechen verb assumes it's "a link"
#. %a1 and %a2 as above, and %a3 is something like (always?) "a link"
msgid "%a1 and %a2 shared %a3."
msgstr "%a1 %a2 a делла %a3."

