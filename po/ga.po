msgid_plural "%d other people"
msgstr[0] "duine amháin eile"
msgstr[1] "beirt eile"
msgstr[2] "%d dhuine eile"
msgstr[3] "%d nduine eile"
msgstr[4] "%d duine eile"

#. %d stands for a number; used in sentences like "%d people like this."
#. The singular "1 person" is never used as far as I can tell
msgid "1 person"
msgid_plural "%d people"
msgstr[0] "duine amháin"
msgstr[1] "beirt"
msgstr[2] "%d dhuine"
msgstr[3] "%d nduine"
msgstr[4] "%d duine"

#. %d stands for a number, no need to translate it
#. The singular "1 second ago" is never used as far as I can tell
msgid "1 second ago"
msgid_plural "%d seconds ago"
msgstr[0] "Soicind ó shin"
msgstr[1] "%d shoicind ó shin"
msgstr[2] "%d shoicind ó shin"
msgstr[3] "%d soicind ó shin"
msgstr[4] "%d soicind ó shin"

#. %d stands for a number, no need to translate it
#. This means a post has been shared %d times
msgid "1 share"
msgid_plural "%d shares"
msgstr[0] "Comhroinnte uair amháin"
msgstr[1] "Comhroinnte %d uair"
msgstr[2] "Comhroinnte %d huaire"
msgstr[3] "Comhroinnte %d n-uaire"
msgstr[4] "Comhroinnte %d uair"

#. Appears at the bottom of your home time line, opposite "More Stories"
